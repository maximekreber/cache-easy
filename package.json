{
    "name": "cache-easy",
    "version": "1.0.2",
    "private": false,
    "main": "dist/src/index.js",
    "scripts": {
        "build": "yarn clean && tsc",
        "clean": "rm -rf dist",
        "test": "jest",
        "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
        "lint": "eslint \"{src,tests}/**/*.ts\"",
        "publish-script": "yarn build && cp package.json ./dist/package.json && cd dist && yarn publish"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/maximekreber/cache-easy.git"
    },
    "keywords": [
        "cache-easy",
        "easy",
        "cache",
        "node-cache",
        "node-easy-cache",
        "node-simple-cache",
        "simple-cache",
        "easy-cache"
    ],
    "author": "Maxime Kreber",
    "license": "ISC",
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^28.1.6",
        "@types/node": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^29.0.3",
        "prettier": "^2.7.1",
        "ts-jest": "^29.0.2",
        "typescript": "^4.7.4"
    }
}
